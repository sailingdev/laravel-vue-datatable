{
  "name": "laravel-vue-datatable",
  "version": "0.1.21",
  "description": "Vue.js datatable made with Laravel and Bootstrap in mind",
  "author": "James Dordoy <jamesdordoy@gmail.com>",
  "homepage": "https://github.com/jamesdordoy/vue-datatable",
  "private": false,
  "bundleDependencies": false,
  "main": "./dist/laravel-vue-datatable.common.js",
  "keywords": [
    "vue",
    "component",
    "bootstrap",
    "laravel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesdordoy/vue-datatable.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib src/plugin.js",
    "build-demo": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "laravel-vue-pagination": "^2.3.0",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "vue-jest": "^3.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "bili": "^4.7.4",
    "core-js": "^2.6.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.8.0",
    "popper.js": "^1.15.0",
    "rollup-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  }
}
